			}
		}
	}
		if(killerid != INVALID_PLAYER_ID && GetPlayerState(killerid) == PLAYER_STATE_DRIVER)//Если убийца за рулем.....(то возможно он Дб)
    {
       new vehicleid = GetPlayerVehicleID(killerid);
      if(GetVehicleModel(vehicleid) != 425 && GetVehicleModel(vehicleid) != 432 && GetVehicleModel(vehicleid) != 447 && GetVehicleModel(vehicleid) != 520 && GetVehicleModel(vehicleid) != 476)//Если игрок не за транспортом с Id 425, 432, 447, 520, 476(это стреляющая техника....Апачи, танки и тому подобное) то он по любому ДБ
         {
          //Ниже Пишем ваши наказания=)

           GameTextForPlayer(killerid, "~R~Drive By", 5000, 1);//Выводит GameText
            SCM(killerid, COLOR_RED, "{FF0000}[Анти-ДБ] {FF6600}Внимание! Убийства машиной запрещены!");//Сообщение в чат
            Kick(playerid);
           return true;
         }
      }
	if(WarTimeSec > 0)
	{
		if(PlayerInfo[playerid][pMember] == Command[0] || PlayerInfo[playerid][pLeader] == Command[0])
		{
			if(PlayerInfo[killerid][pMember] == Command[1] || PlayerInfo[killerid][pLeader] == Command[1])
			{
				CommandKill[1]++;
				new string[12];
				format(string, sizeof(string),"%d",CommandKill[1]);
				TextDrawSetString(kolone, string);
			}
		}
		if(PlayerInfo[playerid][pMember] == Command[1] || PlayerInfo[playerid][pLeader] == Command[1])
		{
			if(PlayerInfo[killerid][pMember] == Command[0] || PlayerInfo[killerid][pLeader] == Command[0])
			{
				CommandKill[0]++;
				new string[12];
				format(string, sizeof(string),"%d",CommandKill[0]);
				TextDrawSetString(koltwo, string);
			}
		}
	}
	if(PlayerInfo[killerid][pJailed] == 0)
	{
		if(IsACop(killerid)) { }
		else
		{
		    if(IsPlayerInAnyVehicle(killerid))
		    {
		        if(PlayerInfo[killerid][pZvezd] == 6) return false;
				PlayerInfo[killerid][pZvezd] += 2;
				if(PlayerInfo[killerid][pZvezd] > 6) PlayerInfo[killerid][pZvezd] = 6;
				SCM(killerid,COLOR_YELLOW,"{FF6600}Вы убили человека из транспорта. Ваш уровень розыска был увеличен");
				SetPlayerWantedLevel(killerid, PlayerInfo[killerid][pZvezd]);
				Kick(killerid);
		    }
		    else
		    {
				if(PlayerInfo[killerid][pZvezd] == 6) return false;
				PlayerInfo[killerid][pZvezd] += 1;
				SCM(killerid,COLOR_YELLOW,"{FF6600}Вы убили человека. Ваш уровень розыска был увеличен");
				SetPlayerWantedLevel(killerid, PlayerInfo[killerid][pZvezd]);
			}
		}
	}
	
	plafk[playerid] = -2;
	new gag[MAX_PLAYERS];
	gag[playerid] = 0;
	if(PlayerInfo[playerid][pLeader] && PlayerInfo[playerid][pMember]) { death[playerid] = 1; }
	EndPlayerCasualJob(playerid);
	death[playerid] = 1;
	passs[playerid] = 0;
	SetPVarInt(playerid,"repair",0);
	SetPVarInt(playerid,"refill",0);
	return true;}
