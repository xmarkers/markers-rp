stock Float:GetDistancePlayers(player1, player2){
	new Float:x1,Float:y1,Float:z1;
	new Float:x2,Float:y2,Float:z2;
	GetPlayerPos(player1, x1, y1, z1);
	GetPlayerPos(player2, x2, y2, z2);
	return floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)
	+floatpower(floatabs(floatsub(y2,y1)),2)
	+floatpower(floatabs(floatsub(z2,z1)),2));}
