stock strrest(const string[], &index){
	new length = strlen(string);
	while((index < length) && (string[index] <= ' ')) index += 1;
	new offset = index;
	new result[128];
	while((index < length) && ((index-offset) < (sizeof(result)-1))) result[index - offset] = string[index], index += 1;
	result[index-offset] = EOS;
	return result;}
