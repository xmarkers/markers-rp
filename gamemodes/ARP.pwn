stock ConvertSeconds(tim){
	new string[128];
	if(tim < 60) format(string, sizeof(string), "{FF0000}%d секунд", tim);
	else if(tim == 60) string = "{FF0000}1 минуту";
	else if(tim > 60 && tim < 3600){
		new Float: minutes;
		new seconds;
		minutes = tim / 60;
		seconds = tim % 60;
		format(string, sizeof(string), "{FF0000}%.0f:%d", minutes, seconds);}
	else if(tim == 3600) string = "{FF0000}1 час";
	else if(tim > 3600){
		new Float: hours;
		new minutes_int;
		new Float: minutes;
		new seconds;
		hours = tim / 3600;
		minutes_int = tim % 3600;
		minutes = minutes_int / 60;
		seconds = minutes_int % 60;
		format(string, sizeof(string), "%.0f:%.0f:%d", hours, minutes, seconds);}
	return string;}
